use assembly

add    teststring,"Hello world!"         ; create variable
mov    ebx,'@teststring"                 ; ebx handles sending message feature

mov    eax,1                             ; mimicks the way we write assembly code
int    0x80                              ; also micks the way we write assembly code


mov    teststring,rga                    ;
rga    apd,"hello"                       ; "Hello world!" + "hello"
mov    eax,3                             ; mimicks message.channel.send()
int    0x80                              ; 
